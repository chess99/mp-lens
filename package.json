{
  "name": "mp-lens",
  "version": "0.1.3",
  "description": "微信小程序分析工具 (Unused Code, Dependencies, Visualization)",
  "main": "dist/index.js",
  "bin": {
    "mp-lens": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && mkdir -p dist/visualizer && cp src/visualizer/template-tree.html src/visualizer/template-graph.html src/visualizer/render-tree.js src/visualizer/render-graph.js dist/visualizer/",
    "watch": "tsc --watch",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "keywords": [
    "wechat",
    "miniprogram",
    "dependency",
    "analyzer",
    "cleaner",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@antv/g6": "^5.0.45",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "d3": "^7.8.2",
    "glob": "^8.1.0",
    "inquirer": "^8.2.5",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/glob": "^8.0.1",
    "@types/inquirer": "^8.2.5",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.19.86",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
