{
  "name": "mp-lens",
  "version": "0.1.13",
  "description": "微信小程序分析工具 (Unused Code, Dependencies, Visualization)",
  "main": "dist/cli/index.js",
  "types": "dist/cli/index.d.ts",
  "bin": {
    "mp-lens": "dist/cli/cli.js"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jest",
    "build:ui": "vite build --config src/ui/vite.config.mjs",
    "build:cli": "tsc -p tsconfig.json",
    "build": "npm run build:ui && npm run build:cli",
    "start": "ts-node src/cli.ts",
    "dev": "npm run build:ui && ts-node src/cli.ts",
    "prepare": "npm run build",
    "clean": "rimraf dist",
    "knip:fix": "npx knip --fix",
    "release": "bash scripts/publish.sh"
  },
  "keywords": [
    "miniprogram",
    "wechat",
    "weixin",
    "uniapp",
    "taro",
    "dependencies",
    "graph",
    "visualization",
    "analyzer",
    "unused code"
  ],
  "author": "chess99",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chess99/mp-lens.git"
  },
  "bugs": {
    "url": "https://github.com/chess99/mp-lens/issues"
  },
  "homepage": "https://github.com/chess99/mp-lens#readme",
  "dependencies": {
    "@antv/g6": "^4.8.21",
    "@wxml/parser": "^0.4.0",
    "chalk": "^4.1.2",
    "chart.js": "^4.4.9",
    "commander": "^12.1.0",
    "glob": "^10.4.1",
    "inquirer": "^12.6.0",
    "jscpd": "^4.0.5",
    "minimatch": "^9.0.4",
    "modern-ahocorasick": "^2.0.4",
    "preact": "^10.22.1",
    "purgecss": "^5.0.0",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.10.1",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.1",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^6.3.3"
  }
}
